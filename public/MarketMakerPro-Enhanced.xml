<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="entry_digit">Entry Digit</variable>
    <variable id="loop_stop">Loop Stop</variable>
    <variable id="stake">Stake</variable>
    <variable id="initial_stake">Initial Stake</variable>
    <variable id="loss_counter">Loss Counter</variable>
    <variable id="martingale_multiplier">Martingale Multiplier</variable>
    <variable id="martingale_level">Martingale Level</variable>
    <variable id="take_profit">Take Profit</variable>
    <variable id="stop_loss">Stop Loss</variable>
    <variable id="text_notify">text_notify</variable>
  </variables>
  <block type="trade_definition" id="trade_def_main" deletable="false" x="0" y="60">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id="market_def" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ50V</field>
        <next>
          <block type="trade_definition_tradetype" id="tradetype_def" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            <next>
              <block type="trade_definition_contracttype" id="contract_def" deletable="false" movable="false">
                <field name="TYPE_LIST">both</field>
                <next>
                  <block type="trade_definition_candleinterval" id="candle_def" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" id="restart_def" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" id="error_def" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="INITIALIZATION">
      <block type="text_print" id="welcome_msg">
        <value name="TEXT">
          <shadow type="text" id="welcome_text">
            <field name="TEXT">ðŸš€ MarketMaker Pro Enhanced - Waiting for Entry Signal...</field>
          </shadow>
        </value>
        <next>
          <block type="variables_set" id="set_entry_digit">
            <field name="VAR" id="entry_digit">Entry Digit</field>
            <value name="VALUE">
              <block type="text_prompt_ext" id="prompt_entry">
                <field name="TYPE">TEXT</field>
                <value name="TEXT">
                  <shadow type="text" id="prompt_text">
                    <field name="TEXT">Enter your entry digit (0-9):</field>
                  </shadow>
                </value>
              </block>
            </value>
            <next>
              <block type="variables_set" id="set_initial_stake">
                <field name="VAR" id="initial_stake">Initial Stake</field>
                <value name="VALUE">
                  <block type="math_number" id="initial_stake_val">
                    <field name="NUM">1</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set" id="set_stake">
                    <field name="VAR" id="stake">Stake</field>
                    <value name="VALUE">
                      <block type="math_number" id="stake_val">
                        <field name="NUM">1</field>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set" id="set_martingale">
                        <field name="VAR" id="martingale_multiplier">Martingale Multiplier</field>
                        <value name="VALUE">
                          <block type="math_number" id="martingale_val">
                            <field name="NUM">2.1</field>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set" id="set_martingale_level">
                            <field name="VAR" id="martingale_level">Martingale Level</field>
                            <value name="VALUE">
                              <block type="math_number" id="martingale_level_val">
                                <field name="NUM">9</field>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set" id="set_take_profit">
                                <field name="VAR" id="take_profit">Take Profit</field>
                                <value name="VALUE">
                                  <block type="math_number" id="take_profit_val">
                                    <field name="NUM">50</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="variables_set" id="set_stop_loss">
                                    <field name="VAR" id="stop_loss">Stop Loss</field>
                                    <value name="VALUE">
                                      <block type="math_number" id="stop_loss_val">
                                        <field name="NUM">100</field>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="variables_set" id="set_loop_stop">
                                        <field name="VAR" id="loop_stop">Loop Stop</field>
                                        <value name="VALUE">
                                          <block type="logic_boolean" id="loop_stop_val">
                                            <field name="BOOL">FALSE</field>
                                          </block>
                                        </value>
                                        <next>
                                          <block type="variables_set" id="set_loss_counter">
                                            <field name="VAR" id="loss_counter">Loss Counter</field>
                                            <value name="VALUE">
                                              <block type="math_number" id="loss_counter_val">
                                                <field name="NUM">0</field>
                                              </block>
                                            </value>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="SUBMARKET">
      <block type="controls_whileUntil" id="wait_loop">
        <field name="MODE">UNTIL</field>
        <value name="BOOL">
          <block type="logic_compare" id="check_loop_stop">
            <field name="OP">EQ</field>
            <value name="A">
              <block type="variables_get" id="get_loop_stop">
                <field name="VAR" id="loop_stop">Loop Stop</field>
              </block>
            </value>
            <value name="B">
              <block type="logic_boolean" id="loop_stop_true">
                <field name="BOOL">TRUE</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO">
          <block type="timeout" id="timeout_block">
            <statement name="TIMEOUTSTACK">
              <block type="controls_if" id="check_entry_digit">
                <value name="IF0">
                  <block type="logic_compare" id="compare_digit">
                    <field name="OP">EQ</field>
                    <value name="A">
                      <block type="last_digit" id="get_last_digit"></block>
                    </value>
                    <value name="B">
                      <block type="variables_get" id="get_entry_digit">
                        <field name="VAR" id="entry_digit">Entry Digit</field>
                      </block>
                    </value>
                  </block>
                </value>
                <statement name="DO0">
                  <block type="variables_set" id="stop_loop">
                    <field name="VAR" id="loop_stop">Loop Stop</field>
                    <value name="VALUE">
                      <block type="logic_boolean" id="stop_loop_true">
                        <field name="BOOL">TRUE</field>
                      </block>
                    </value>
                    <next>
                      <block type="text_join" id="entry_found_msg">
                        <field name="VARIABLE" id="text_notify">text_notify</field>
                        <statement name="STACK">
                          <block type="text_statement" id="msg_part1">
                            <value name="TEXT">
                              <shadow type="text" id="msg_text1">
                                <field name="TEXT">âœ… Entry Signal Found! Digit </field>
                              </shadow>
                            </value>
                            <next>
                              <block type="text_statement" id="msg_part2">
                                <value name="TEXT">
                                  <shadow type="text" id="msg_text2">
                                    <field name="TEXT"></field>
                                  </shadow>
                                  <block type="variables_get" id="get_entry_for_msg">
                                    <field name="VAR" id="entry_digit">Entry Digit</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="text_statement" id="msg_part3">
                                    <value name="TEXT">
                                      <shadow type="text" id="msg_text3">
                                        <field name="TEXT"> detected. Starting trades...</field>
                                      </shadow>
                                    </value>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </statement>
                        <next>
                          <block type="notify" id="notify_entry">
                            <field name="NOTIFICATION_TYPE">success</field>
                            <field name="NOTIFICATION_SOUND">earned-money</field>
                            <value name="MESSAGE">
                              <shadow type="text" id="notify_shadow">
                                <field name="TEXT">abc</field>
                              </shadow>
                              <block type="variables_get" id="get_notify_text">
                                <field name="VAR" id="text_notify">text_notify</field>
                              </block>
                            </value>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </statement>
              </block>
            </statement>
            <value name="SECONDS">
              <block type="math_number" id="timeout_seconds">
                <field name="NUM">1</field>
              </block>
            </value>
          </block>
        </statement>
        <next>
          <block type="trade_definition_tradeoptions" id="trade_options">
            <mutation xmlns="http://www.w3.org/1999/xhtml" has_first_barrier="false" has_second_barrier="false" has_prediction="true"></mutation>
            <field name="DURATIONTYPE_LIST">t</field>
            <value name="DURATION">
              <shadow type="math_number_positive" id="duration_shadow">
                <field name="NUM">1</field>
              </shadow>
              <block type="math_number" id="duration_val">
                <field name="NUM">1</field>
              </block>
            </value>
            <value name="AMOUNT">
              <shadow type="math_number_positive" id="amount_shadow">
                <field name="NUM">1</field>
              </shadow>
              <block type="variables_get" id="get_stake">
                <field name="VAR" id="stake">Stake</field>
              </block>
            </value>
            <value name="PREDICTION">
              <shadow type="math_number_positive" id="prediction_shadow">
                <field name="NUM">1</field>
              </shadow>
              <block type="logic_ternary" id="prediction_logic">
                <value name="IF">
                  <block type="logic_compare" id="check_loss_0">
                    <field name="OP">EQ</field>
                    <value name="A">
                      <block type="variables_get" id="get_loss_0">
                        <field name="VAR" id="loss_counter">Loss Counter</field>
                      </block>
                    </value>
                    <value name="B">
                      <block type="math_number" id="loss_0_val">
                        <field name="NUM">0</field>
                      </block>
                    </value>
                  </block>
                </value>
                <value name="THEN">
                  <block type="math_number" id="predict_8">
                    <field name="NUM">8</field>
                  </block>
                </value>
                <value name="ELSE">
                  <block type="math_number" id="predict_5">
                    <field name="NUM">5</field>
                  </block>
                </value>
              </block>
            </value>
          </block>
        </next>
      </block>
    </statement>
  </block>
  <block type="after_purchase" id="after_purchase_main" x="1000" y="60">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if" id="check_profit_loss">
        <mutation xmlns="http://www.w3.org/1999/xhtml" elseif="1" else="1"></mutation>
        <value name="IF0">
          <block type="logic_compare" id="check_take_profit">
            <field name="OP">GTE</field>
            <value name="A">
              <block type="total_profit" id="get_total_profit_1"></block>
            </value>
            <value name="B">
              <block type="variables_get" id="get_take_profit">
                <field name="VAR" id="take_profit">Take Profit</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <block type="text_print" id="print_take_profit">
            <value name="TEXT">
              <shadow type="text" id="take_profit_text">
                <field name="TEXT">ðŸŽ¯ Take Profit Reached! Stopping bot...</field>
              </shadow>
            </value>
            <next>
              <block type="notify" id="notify_take_profit">
                <field name="NOTIFICATION_TYPE">success</field>
                <field name="NOTIFICATION_SOUND">earned-money</field>
                <value name="MESSAGE">
                  <shadow type="text" id="notify_tp_shadow">
                    <field name="TEXT">ðŸŽ¯ Take Profit Reached!</field>
                  </shadow>
                </value>
              </block>
            </next>
          </block>
        </statement>
        <value name="IF1">
          <block type="logic_compare" id="check_stop_loss">
            <field name="OP">LTE</field>
            <value name="A">
              <block type="total_profit" id="get_total_profit_2"></block>
            </value>
            <value name="B">
              <block type="math_single" id="negate_stop_loss">
                <field name="OP">NEG</field>
                <value name="NUM">
                  <shadow type="math_number" id="stop_loss_shadow">
                    <field name="NUM">9</field>
                  </shadow>
                  <block type="variables_get" id="get_stop_loss">
                    <field name="VAR" id="stop_loss">Stop Loss</field>
                  </block>
                </value>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO1">
          <block type="text_print" id="print_stop_loss">
            <value name="TEXT">
              <shadow type="text" id="stop_loss_text">
                <field name="TEXT">ðŸ›‘ Stop Loss Reached! Stopping bot...</field>
              </shadow>
            </value>
            <next>
              <block type="notify" id="notify_stop_loss">
                <field name="NOTIFICATION_TYPE">error</field>
                <field name="NOTIFICATION_SOUND">error</field>
                <value name="MESSAGE">
                  <shadow type="text" id="notify_sl_shadow">
                    <field name="TEXT">ðŸ›‘ Stop Loss Reached!</field>
                  </shadow>
                </value>
              </block>
            </next>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="controls_if" id="check_win_loss">
            <mutation xmlns="http://www.w3.org/1999/xhtml" else="1"></mutation>
            <value name="IF0">
              <block type="contract_check_result" id="check_win">
                <field name="CHECK_RESULT">win</field>
              </block>
            </value>
            <statement name="DO0">
              <block type="variables_set" id="reset_stake">
                <field name="VAR" id="stake">Stake</field>
                <value name="VALUE">
                  <block type="variables_get" id="get_initial_stake">
                    <field name="VAR" id="initial_stake">Initial Stake</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set" id="reset_loss_counter">
                    <field name="VAR" id="loss_counter">Loss Counter</field>
                    <value name="VALUE">
                      <block type="math_number" id="reset_loss_val">
                        <field name="NUM">0</field>
                      </block>
                    </value>
                  </block>
                </next>
              </block>
            </statement>
            <statement name="ELSE">
              <block type="controls_if" id="check_martingale_limit">
                <value name="IF0">
                  <block type="logic_compare" id="compare_martingale">
                    <field name="OP">LT</field>
                    <value name="A">
                      <block type="variables_get" id="get_loss_counter">
                        <field name="VAR" id="loss_counter">Loss Counter</field>
                      </block>
                    </value>
                    <value name="B">
                      <block type="variables_get" id="get_martingale_level">
                        <field name="VAR" id="martingale_level">Martingale Level</field>
                      </block>
                    </value>
                  </block>
                </value>
                <statement name="DO0">
                  <block type="variables_set" id="increase_stake">
                    <field name="VAR" id="stake">Stake</field>
                    <value name="VALUE">
                      <block type="math_arithmetic" id="calc_new_stake">
                        <field name="OP">MULTIPLY</field>
                        <value name="A">
                          <shadow type="math_number" id="stake_shadow">
                            <field name="NUM">1</field>
                          </shadow>
                          <block type="variables_get" id="get_current_stake">
                            <field name="VAR" id="stake">Stake</field>
                          </block>
                        </value>
                        <value name="B">
                          <shadow type="math_number" id="multiplier_shadow">
                            <field name="NUM">1</field>
                          </shadow>
                          <block type="variables_get" id="get_multiplier">
                            <field name="VAR" id="martingale_multiplier">Martingale Multiplier</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set" id="increment_loss">
                        <field name="VAR" id="loss_counter">Loss Counter</field>
                        <value name="VALUE">
                          <block type="math_arithmetic" id="calc_loss_counter">
                            <field name="OP">ADD</field>
                            <value name="A">
                              <shadow type="math_number" id="loss_add_shadow">
                                <field name="NUM">1</field>
                              </shadow>
                              <block type="variables_get" id="get_loss_for_add">
                                <field name="VAR" id="loss_counter">Loss Counter</field>
                              </block>
                            </value>
                            <value name="B">
                              <shadow type="math_number" id="add_one">
                                <field name="NUM">1</field>
                              </shadow>
                            </value>
                          </block>
                        </value>
                      </block>
                    </next>
                  </block>
                </statement>
              </block>
            </statement>
            <next>
              <block type="trade_again" id="trade_again_block"></block>
            </next>
          </block>
        </statement>
      </block>
    </statement>
  </block>
  <block type="before_purchase" id="before_purchase_main" deletable="false" x="0" y="1500">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="controls_if" id="check_loss_before_purchase">
        <value name="IF0">
          <block type="logic_compare" id="check_loss_limit">
            <field name="OP">LTE</field>
            <value name="A">
              <block type="variables_get" id="get_loss_before">
                <field name="VAR" id="loss_counter">Loss Counter</field>
              </block>
            </value>
            <value name="B">
              <block type="variables_get" id="get_level_before">
                <field name="VAR" id="martingale_level">Martingale Level</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <block type="purchase" id="purchase_block">
            <field name="PURCHASE_LIST">DIGITOVER</field>
          </block>
        </statement>
      </block>
    </statement>
  </block>
</xml>
